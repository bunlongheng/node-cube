# .env.example

# Database type
CUBEJS_DB_TYPE=snowflake

# Snowflake-specific settings
CUBEJS_DB_SNOWFLAKE_ACCOUNT=your_account_region_id
CUBEJS_DB_SNOWFLAKE_WAREHOUSE=your_warehouse_name
CUBEJS_DB_SNOWFLAKE_SCHEMA=your_schema_name

# Generic connection settings
CUBEJS_DB_NAME=your_database_name
CUBEJS_DB_USER=your_username
CUBEJS_DB_PASS=your_password

# Cube.js
CUBEJS_API_SECRET=your_api_secret

# Cache & queue (optional)
CUBEJS_CACHE_AND_QUEUE_DRIVER=memory
CUBEJS_REDIS_URL=redis://your_redis_host:6379